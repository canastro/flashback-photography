{
    "presets": [["env", { "targets": { "browsers": ["IE >= 11"] } }], "react"],
    "plugins": [
        "add-module-exports",
        "transform-class-properties",
        "transform-decorators-legacy",
        "transform-flow-strip-types"
    ],
    "env": {
        "development": {
            "plugins": [
                [
                    "flow-runtime",
                    {
                        "assert": true,
                        "annotate": true
                    }
                ]
            ]
        }
    }
}
